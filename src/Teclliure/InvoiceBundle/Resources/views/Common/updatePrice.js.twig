$('#total_base_amount').html('{{ common.baseAmount|number_format(2, '.', ',') }}€');
$('#total_discount').html('{{ common.discountAmount|number_format(2, '.', ',') }}€');
$('#total_subtotal').html('{{ common.netAmount|number_format(2, '.', ',') }}€');
$('#total_taxes').html('{{ common.taxAmount|number_format(2, '.', ',') }}€');
$('#total_total').html('{{ common.grossAmount|number_format(2, '.', ',') }}€');
{% for key,line in lines %}
$('#{{ key }}').find('.linePrice').html('{{ line.totalAmount|number_format(2, '.', ',') }}€');
{% endfor %}
